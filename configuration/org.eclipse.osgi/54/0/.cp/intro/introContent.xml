<introContent>
	<!-- Root page -->
	<page id="root" alt-style="" style="style.css" style-id="page">
        <group id="content">
            <group id="welcome-text">
                <img src="img/welcome_txt.png"/>
            </group>
	        <group id="action-links">
	          <group id="first">
                <link url="http://org.eclipse.ui.intro/runAction?pluginId=dreisoft.tresos.application.launcher2.plugin&amp;class=dreisoft.tresos.application.launcher2.platform.OpenDocIntroAction&amp;docFile=doc/2.0_EB_tresos_Studio/2.3_Studio_new_and_noteworthy.pdf" id="whatsnew">
                  <text>Press this link button to find out what is new in this release.</text>
                </link>
                <link url="http://org.eclipse.ui.intro/runAction?pluginId=dreisoft.tresos.application.launcher2.plugin&amp;class=dreisoft.tresos.application.launcher2.platform.OpenDocIntroAction&amp;docFile=doc/2.0_EB_tresos_Studio/2.2_Studio_release_notes.pdf" id="releasenotes">
                  <text>Press this link button to open the release notes.</text>
                </link>
	            <link url="http://org.eclipse.ui.intro/showHelp" id="help">
	              <text>Press this link button to open the online help.</text>
	            </link>
                <link url="http://org.eclipse.ui.intro/close" id="start">
                  <text>Press this link button to close this welcome screen.</text>
                </link>
	          </group>
	          <group id="second">
                <link url="http://org.eclipse.ui.intro/runAction?class=dreisoft.tresos.launcher2.gui.actions.LicenseAction&amp;pluginId=dreisoft.tresos.launcher2.plugin&amp;standby=true" id="license">
                  <text>Press this link button to open the Licenses dialog.</text>
                </link>
                <link url="http://org.eclipse.ui.intro/openBrowser?url=http://automotive.elektrobit.com/support/" id="supportrequest">
                  <text>Press this link button to issue a support request.</text>
                </link>
                <link url="http://org.eclipse.ui.intro/openBrowser?url=http://automotive.elektrobit.com/trainings/" id="training">
                  <text>Press this link button to find info about Automotive Technology Training Courses.</text>
                </link>
	          </group>
	        </group>
        </group>
	</page>

	<!-- Standby page -->
	<page id="standby" alt-style="" style="style.css" style-id="page">
		<anchor id="head-anchor" />
		<group id="standby-title">
			<text>EB tresos</text>
		</group>

		<group id="standby-links">
           <group>
               <link url="http://org.eclipse.ui.intro/setStandbyMode?standby=false"
                label="Show welcome screen" />
           </group>
           <group>
              <link url="http://org.eclipse.ui.intro/openBrowser?url=http://automotive.elektrobit.com/support/"
                label="Create support request" />
           </group>
	       <group>
              <link url="http://org.eclipse.ui.intro/openBrowser?url=http://automotive.elektrobit.com/home/ecu-software/eb-tresos-product-line.html"
                label="Navigate to EB tresos homepage" />
	       </group>
           <group>
              <link url="http://org.eclipse.ui.intro/runAction?class=dreisoft.tresos.launcher2.gui.actions.LicenseAction&amp;pluginId=dreisoft.tresos.launcher2.plugin&amp;standby=true"
                label="Open the License preferences" />
           </group>
	       
		</group>
	</page>

</introContent>
